<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.com.diaryapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:label="DiaryMemo"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:icon="@mipmap/ic_icon">
        <activity android:name=".HomeActivity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="DiaryMemo"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".DiaryWriteActivity"
            android:label="일기쓰기"></activity>
        <activity android:name=".DiaryActivity"
            android:label="일기장"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MemoActivity"
            android:label="Memo"></activity>
        <activity android:name=".SignupPage"
            android:label="Signup"></activity>

    </application>

</manifest>